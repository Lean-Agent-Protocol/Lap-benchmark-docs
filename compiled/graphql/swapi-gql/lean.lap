@lap v0.1
@api Swapi Gql
@base /graphql

type Film:Node{title:str,episodeID:int,openingCrawl:str,director:str,producers:[str],releaseDate:str,speciesConnection(@page):FilmSpeciesConnection,starshipConnection(@page):FilmStarshipsConnection,vehicleConnection(@page):FilmVehiclesConnection,characterConnection(@page):FilmCharactersConnection,planetConnection(@page):FilmPlanetsConnection,created:str,edited:str}
type FilmCharactersConnection{pageInfo:PageInfo!,edges:[FilmCharactersEdge],totalCount:int,characters:[Person]}
type FilmPlanetsConnection{pageInfo:PageInfo!,edges:[FilmPlanetsEdge],totalCount:int,planets:[Planet]}
type FilmSpeciesConnection{pageInfo:PageInfo!,edges:[FilmSpeciesEdge],totalCount:int,species:[Species]}
type FilmStarshipsConnection{pageInfo:PageInfo!,edges:[FilmStarshipsEdge],totalCount:int,starships:[Starship]}
type FilmVehiclesConnection{pageInfo:PageInfo!,edges:[FilmVehiclesEdge],totalCount:int,vehicles:[Vehicle]}
type FilmsConnection{pageInfo:PageInfo!,edges:[FilmsEdge],totalCount:int,films:[Film]}
iface Node{id:id!}
type PageInfo{hasNextPage:bool!,hasPreviousPage:bool!,startCursor:str,endCursor:str}
type PeopleConnection{pageInfo:PageInfo!,edges:[PeopleEdge],totalCount:int,people:[Person]}
type Person:Node{name:str,birthYear:str,eyeColor:str,gender:str,hairColor:str,height:int,mass:num,skinColor:str,homeworld:Planet,filmConnection(@page):PersonFilmsConnection,species:Species,starshipConnection(@page):PersonStarshipsConnection,vehicleConnection(@page):PersonVehiclesConnection,created:str,edited:str}
type PersonFilmsConnection{pageInfo:PageInfo!,edges:[PersonFilmsEdge],totalCount:int,films:[Film]}
type PersonStarshipsConnection{pageInfo:PageInfo!,edges:[PersonStarshipsEdge],totalCount:int,starships:[Starship]}
type PersonVehiclesConnection{pageInfo:PageInfo!,edges:[PersonVehiclesEdge],totalCount:int,vehicles:[Vehicle]}
type Planet:Node{name:str,diameter:int,rotationPeriod:int,orbitalPeriod:int,gravity:str,population:num,climates:[str],terrains:[str],surfaceWater:num,residentConnection(@page):PlanetResidentsConnection,filmConnection(@page):PlanetFilmsConnection,created:str,edited:str}
type PlanetFilmsConnection{pageInfo:PageInfo!,edges:[PlanetFilmsEdge],totalCount:int,films:[Film]}
type PlanetResidentsConnection{pageInfo:PageInfo!,edges:[PlanetResidentsEdge],totalCount:int,residents:[Person]}
type PlanetsConnection{pageInfo:PageInfo!,edges:[PlanetsEdge],totalCount:int,planets:[Planet]}
type Species:Node{name:str,classification:str,designation:str,averageHeight:num,averageLifespan:int,eyeColors:[str],hairColors:[str],skinColors:[str],language:str,homeworld:Planet,personConnection(@page):SpeciesPeopleConnection,filmConnection(@page):SpeciesFilmsConnection,created:str,edited:str}
type SpeciesConnection{pageInfo:PageInfo!,edges:[SpeciesEdge],totalCount:int,species:[Species]}
type SpeciesFilmsConnection{pageInfo:PageInfo!,edges:[SpeciesFilmsEdge],totalCount:int,films:[Film]}
type SpeciesPeopleConnection{pageInfo:PageInfo!,edges:[SpeciesPeopleEdge],totalCount:int,people:[Person]}
type Starship:Node{name:str,model:str,starshipClass:str,manufacturers:[str],costInCredits:num,length:num,crew:str,passengers:str,maxAtmospheringSpeed:int,hyperdriveRating:num,MGLT:int,cargoCapacity:num,consumables:str,pilotConnection(@page):StarshipPilotsConnection,filmConnection(@page):StarshipFilmsConnection,created:str,edited:str}
type StarshipFilmsConnection{pageInfo:PageInfo!,edges:[StarshipFilmsEdge],totalCount:int,films:[Film]}
type StarshipPilotsConnection{pageInfo:PageInfo!,edges:[StarshipPilotsEdge],totalCount:int,pilots:[Person]}
type StarshipsConnection{pageInfo:PageInfo!,edges:[StarshipsEdge],totalCount:int,starships:[Starship]}
type Vehicle:Node{name:str,model:str,vehicleClass:str,manufacturers:[str],costInCredits:num,length:num,crew:str,passengers:str,maxAtmospheringSpeed:int,cargoCapacity:num,consumables:str,pilotConnection(@page):VehiclePilotsConnection,filmConnection(@page):VehicleFilmsConnection,created:str,edited:str}
type VehicleFilmsConnection{pageInfo:PageInfo!,edges:[VehicleFilmsEdge],totalCount:int,films:[Film]}
type VehiclePilotsConnection{pageInfo:PageInfo!,edges:[VehiclePilotsEdge],totalCount:int,pilots:[Person]}
type VehiclesConnection{pageInfo:PageInfo!,edges:[VehiclesEdge],totalCount:int,vehicles:[Vehicle]}

Q allFilms(after:str,first:int,before:str,last:int)->FilmsConnection
Q film(id:id,filmID:id)->Film
Q allPeople(after:str,first:int,before:str,last:int)->PeopleConnection
Q person(id:id,personID:id)->Person
Q allPlanets(after:str,first:int,before:str,last:int)->PlanetsConnection
Q planet(id:id,planetID:id)->Planet
Q allSpecies(after:str,first:int,before:str,last:int)->SpeciesConnection
Q species(id:id,speciesID:id)->Species
Q allStarships(after:str,first:int,before:str,last:int)->StarshipsConnection
Q starship(id:id,starshipID:id)->Starship
Q allVehicles(after:str,first:int,before:str,last:int)->VehiclesConnection
Q vehicle(id:id,vehicleID:id)->Vehicle
Q node(id:id!)->Node
