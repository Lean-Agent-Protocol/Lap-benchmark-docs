@doclean v0.1
@api Wordpress
@base /graphql

type Avatar{url:str,size:int,foundAvatar:bool!}
enum AvatarRatingEnum=G|PG|R|X
type Category:Node{id:id!,databaseId:int!,name:str,slug:str,description:str,count:int,parentId:id,parent:Category,children(first:int,after:str):CategoryToCategoryConnection,posts(first:int,after:str):CategoryToPostConnection}
type CategoryToCategoryConnection{edges:[CategoryToCategoryConnectionEdge!],pageInfo:PageInfo!}
type CategoryToCategoryConnectionEdge{cursor:str,node:Category}
type CategoryToPostConnection{edges:[CategoryToPostConnectionEdge!],pageInfo:PageInfo!}
type CategoryToPostConnectionEdge{cursor:str,node:Post}
type Comment:Node{id:id!,databaseId:int!,content(format:str):str,date:str,status:CommentStatusEnum,author:CommentAuthor,parentId:id,parent:Comment,replies(first:int,after:str):CommentToCommentConnection,commentedOn:ContentNode}
union CommentAuthor=User|CommentAuthorExternal
type CommentAuthorExternal{name:str,email:str,url:str}
enum CommentStatusEnum=APPROVE|HOLD|SPAM|TRASH
type CommentToCommentConnection{edges:[CommentToCommentConnectionEdge!],pageInfo:PageInfo!}
type CommentToCommentConnectionEdge{cursor:str,node:Comment}
iface ContentNode{id:id!,databaseId:int!,slug:str,status:str,date:str,modified:str,uri:str}
input CreateCommentInput{commentOn:int!,content:str!,author:str,authorEmail:str,parentId:id}
type CreateCommentPayload{comment:Comment}
input CreatePostInput{title:str!,content:str,excerpt:str,status:PostStatusEnum,categoryIds:[id!],tagIds:[id!]}
type CreatePostPayload{post:Post}
input DateInput{year:int!,month:int,day:int}
input DateQueryInput{after:DateInput,before:DateInput}
type DeletePostPayload{deletedId:id,post:Post}
type GeneralSettings{title:str,description:str,url:str,language:str,timezone:str,dateFormat:str,timeFormat:str}
type MediaDetails{width:int,height:int,file:str,sizes:[MediaSize!]}
type MediaItem:Node,ContentNode{id:id!,databaseId:int!,slug:str,status:str,date:str,modified:str,uri:str,title(format:str):str,altText:str,caption(format:str):str,description(format:str):str,mediaItemUrl:str,sourceUrl(size:MediaItemSizeEnum):str,srcSet(size:MediaItemSizeEnum):str,sizes(size:MediaItemSizeEnum):str,mimeType:str,mediaType:str,fileSize:int,mediaDetails:MediaDetails}
enum MediaItemSizeEnum=THUMBNAIL|MEDIUM|MEDIUM_LARGE|LARGE|FULL
type MediaSize{name:str!,width:str,height:str,sourceUrl:str,mimeType:str}
type Menu:Node{id:id!,databaseId:int!,name:str,slug:str,locations:[str],menuItems(first:int,after:str):MenuToMenuItemConnection}
type MenuItem:Node{id:id!,databaseId:int!,label:str,url:str,parentId:id,order:int,target:str,cssClasses:[str],connectedNode:MenuItemToMenuItemLinkableConnectionEdge}
type MenuItemToMenuItemLinkableConnectionEdge{node:ContentNode}
type MenuToMenuItemConnection{edges:[MenuToMenuItemConnectionEdge!],pageInfo:PageInfo!}
type MenuToMenuItemConnectionEdge{cursor:str,node:MenuItem}
iface Node{id:id!}
iface NodeWithAuthor{authorId:id,authorDatabaseId:int}
type NodeWithAuthorToUserConnectionEdge{node:User}
iface NodeWithComments{commentCount:int,commentStatus:str}
iface NodeWithContentEditor{content(format:str):str}
iface NodeWithExcerpt{excerpt(format:str):str}
iface NodeWithFeaturedImage{featuredImage:NodeWithFeaturedImageToMediaItemConnectionEdge}
type NodeWithFeaturedImageToMediaItemConnectionEdge{node:MediaItem}
iface NodeWithTitle{title(format:str):str}
enum OrderEnum=ASC|DESC
type Page:Node,ContentNode,NodeWithTitle,NodeWithContentEditor,NodeWithFeaturedImage{id:id!,databaseId:int!,slug:str,status:str,date:str,modified:str,uri:str,title(format:str):str,content(format:str):str,featuredImage:NodeWithFeaturedImageToMediaItemConnectionEdge,parentId:id,parent:Page,children(first:int,after:str):PageToPageConnection,menuOrder:int,isFrontPage:bool!,isPrivacyPage:bool!}
type PageInfo{hasNextPage:bool!,hasPreviousPage:bool!,startCursor:str,endCursor:str}
type PageToPageConnection{edges:[PageToPageConnectionEdge!],pageInfo:PageInfo!}
type PageToPageConnectionEdge{cursor:str,node:Page}
type Post:Node,ContentNode,NodeWithTitle,NodeWithContentEditor,NodeWithExcerpt,NodeWithFeaturedImage,NodeWithAuthor,NodeWithComments{id:id!,databaseId:int!,slug:str,status:str,date:str,modified:str,uri:str,title(format:str):str,content(format:str):str,excerpt(format:str):str,featuredImage:NodeWithFeaturedImageToMediaItemConnectionEdge,authorId:id,authorDatabaseId:int,author:NodeWithAuthorToUserConnectionEdge,categories(first:int,after:str,where:PostToCategoryConnectionWhereArgs):PostToCategoryConnection,tags(first:int,after:str):PostToTagConnection,comments(first:int,after:str,where:PostToCommentConnectionWhereArgs):PostToCommentConnection,commentCount:int,commentStatus:str,isSticky:bool!,pingStatus:str,toPing:[str],pinged:[str]}
enum PostObjectsConnectionOrderbyEnum=DATE|MODIFIED|TITLE|AUTHOR|MENU_ORDER|COMMENT_COUNT
input PostObjectsConnectionOrderbyInput{field:PostObjectsConnectionOrderbyEnum!,order:OrderEnum!}
enum PostStatusEnum=PUBLISH|DRAFT|PENDING|PRIVATE|TRASH|FUTURE|AUTO_DRAFT|INHERIT
type PostToCategoryConnection{edges:[PostToCategoryConnectionEdge!],pageInfo:PageInfo!}
type PostToCategoryConnectionEdge{cursor:str,node:Category}
input PostToCategoryConnectionWhereArgs{search:str}
type PostToCommentConnection{edges:[PostToCommentConnectionEdge!],pageInfo:PageInfo!}
type PostToCommentConnectionEdge{cursor:str,node:Comment}
input PostToCommentConnectionWhereArgs{status:CommentStatusEnum}
type PostToTagConnection{edges:[PostToTagConnectionEdge!],pageInfo:PageInfo!}
type PostToTagConnectionEdge{cursor:str,node:Tag}
enum RelationEnum=AND|OR
type RootQueryToCategoryConnection{edges:[RootQueryToCategoryConnectionEdge!],pageInfo:PageInfo!}
type RootQueryToCategoryConnectionEdge{cursor:str,node:Category}
type RootQueryToMediaItemConnection{edges:[RootQueryToMediaItemConnectionEdge!],pageInfo:PageInfo!}
type RootQueryToMediaItemConnectionEdge{cursor:str,node:MediaItem}
type RootQueryToPageConnection{edges:[RootQueryToPageConnectionEdge!],pageInfo:PageInfo!}
type RootQueryToPageConnectionEdge{cursor:str,node:Page}
type RootQueryToPostConnection{edges:[RootQueryToPostConnectionEdge!],pageInfo:PageInfo!}
type RootQueryToPostConnectionEdge{cursor:str,node:Post}
input RootQueryToPostConnectionWhereArgs{status:PostStatusEnum,categoryName:str,tag:str,search:str,author:int,authorName:str,orderby:[PostObjectsConnectionOrderbyInput!],dateQuery:DateQueryInput}
type RootQueryToTagConnection{edges:[RootQueryToTagConnectionEdge!],pageInfo:PageInfo!}
type RootQueryToTagConnectionEdge{cursor:str,node:Tag}
type Tag:Node{id:id!,databaseId:int!,name:str,slug:str,description:str,count:int,posts(first:int,after:str):TagToPostConnection}
type TagToPostConnection{edges:[TagToPostConnectionEdge!],pageInfo:PageInfo!}
type TagToPostConnectionEdge{cursor:str,node:Post}
enum TaxonomyEnum=CATEGORY|TAG
input UpdatePostInput{title:str,content:str,excerpt:str,status:PostStatusEnum,categoryIds:[id!],tagIds:[id!]}
type UpdatePostPayload{post:Post}
type User:Node{id:id!,databaseId:int!,name:str,slug:str,email:str,url:str,description:str,avatar(size:int,rating:AvatarRatingEnum):Avatar,roles:UserToUserRoleConnection,posts(first:int,after:str):UserToPostConnection,pages(first:int,after:str):UserToPageConnection}
type UserRole{name:str,displayName:str}
type UserToPageConnection{edges:[UserToPageConnectionEdge!],pageInfo:PageInfo!}
type UserToPageConnectionEdge{cursor:str,node:Page}
type UserToPostConnection{edges:[UserToPostConnectionEdge!],pageInfo:PageInfo!}
type UserToPostConnectionEdge{cursor:str,node:Post}
type UserToUserRoleConnection{edges:[UserToUserRoleConnectionEdge!]}
type UserToUserRoleConnectionEdge{node:UserRole}

Q post(id:id!,idType:str)->Post
Q posts(first:int,after:str,where:RootQueryToPostConnectionWhereArgs)->RootQueryToPostConnection
Q page(id:id!,idType:str)->Page
Q pages(first:int,after:str)->RootQueryToPageConnection
Q mediaItem(id:id!)->MediaItem
Q mediaItems(first:int,after:str)->RootQueryToMediaItemConnection
Q category(id:id!)->Category
Q categories(first:int,after:str)->RootQueryToCategoryConnection
Q tag(id:id!)->Tag
Q tags(first:int,after:str)->RootQueryToTagConnection
Q user(id:id!)->User
Q viewer->User
Q menu(id:id!)->Menu
Q generalSettings->GeneralSettings
Q node(id:id!)->Node
M createPost(input:CreatePostInput!)->CreatePostPayload
M updatePost(id:id!,input:UpdatePostInput!)->UpdatePostPayload
M deletePost(id:id!,forceDelete:bool)->DeletePostPayload
M createComment(input:CreateCommentInput!)->CreateCommentPayload
